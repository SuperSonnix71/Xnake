{
  "name": "modern-snake-game",
  "version": "1.0.0",
  "description": "A modern, awesome-looking Snake game with Node.js and Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test/ml-anticheat.test.js",
    "ml:train": "node ml/train.js",
    "ml:train:full": "node ml/train.js --epochs=100 --min-samples=200",
    "ml:init": "node scripts/init-ml-versioning.js",
    "ml:review-edges": "node ml/review-edge-cases.js"
  },
  "keywords": [
    "snake",
    "game",
    "nodejs",
    "express",
    "html5",
    "canvas"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "sql.js": "^1.10.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "^25.1.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^17.2.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.9.3"
  }
